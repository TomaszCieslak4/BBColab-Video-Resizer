{
    "name": "BBCollab Video Resizer",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Resizes the Blackboard Collaborate's user video.",
    "content_scripts": [
        {
            "matches": [
                "https://ca.bbcollab.com/collab/ui/session/join/*"
            ],
            "css": [
                "styles.css"
            ],
            "js": [
                "js/contentScript.js"
            ]
        }
    ],
    "browser_action": {        
        "default_popup": "popup.html",
        "default_icon": { 
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "192": "images/192x192.png"
        }
    }

}